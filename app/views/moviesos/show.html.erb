<div class="page-header">
  <h3>Movieso #<%= @movieso.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Source</dt>
      <dd><%= @movieso.source %></dd>

      <dt>Caption</dt>
      <dd><%= @movieso.caption %></dd>

      <dt>Image url</dt>
      <dd><%= @movieso.image_url %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/moviesos" class="btn btn-primary">
        Back
      </a>
      <a href="/moviesos/<%= @movieso.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_movieso/<%= @movieso.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
