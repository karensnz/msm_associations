<div class="page-header">
  <h1>
    Moviesos
    <a href="/moviesos/new" class="btn btn-lg btn-success">New Movieso</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Source</th>
        <th>Caption</th>
        <th>Image url</th>
        <th>Actions</th>
      </tr>

      <% @moviesos.each do |movieso| %>
      <tr>
        <td><%= movieso.source %></td>
        <td><%= movieso.caption %></td>
        <td><%= movieso.image_url %></td>
        <td>
          <a href="/moviesos/<%= movieso.id %>" class="btn btn-primary">Show</a>
          <a href="/moviesos/<%= movieso.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_movieso/<%= movieso.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
